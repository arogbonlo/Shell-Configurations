# last modified 17-05-2017

alias test_internet="ping -c 3 8.8.8.8"
alias vi="vim"
alias clearls="clear; ls -a"
alias sage_jupyter="sage -notebook=jupyter"
alias rm="rm -i"
alias dataenv="cd ~/Documents/klgRepo/jnbs && workon data; jupyter notebook"
